{
  "name": "medicate-backend",
  "version": "0.0.1",
  "description": "Backend api for medicon application",
  "author": "Folkatech",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate:up": "typeorm-ts-node-commonjs migration:run -d ./migrations.ts",
    "migrate:down": "typeorm-ts-node-commonjs migration:revert -d ./migrations.ts",
    "db:seed": "ts-node ./seeds.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.670.0",
    "@aws-sdk/credential-providers": "^3.670.0",
    "@aws-sdk/s3-request-presigner": "^3.670.0",
    "@nestjs/axios": "^3.1.1",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^1.2.1",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.1.1",
    "@nestjs/throttler": "^2.0.1",
    "@nestjs/typeorm": "^9.0.1",
    "@t00nday/nestjs-pdf": "^3.0.5",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "ejs": "^3.1.8",
    "exceljs": "^4.4.0",
    "fastify-multer": "^2.0.3",
    "helmet": "^4.6.0",
    "joi": "^17.7.0",
    "luxon": "^3.5.0",
    "module-alias": "^2.2.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "mysql2": "^3.0.1",
    "nest-winston": "^1.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "puppeteer": "^23.5.3",
    "qrcode": "^1.5.1",
    "randomstring": "^1.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "xendit-node": "^6.3.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.4",
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jest": "27.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.18.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "casual": "^1.6.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.3",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "typeorm-extension": "^2.4.2",
    "typeorm-seeding": "^1.6.1",
    "typescript": "~4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "@common/(.*)": "<rootDir>/common/$1",
      "@config/(.*)": "<rootDir>/config/$1",
      "@database/(.*)": "<rootDir>/database/$1",
      "@entities/(.*)": "<rootDir>/entities/$1",
      "@models/(.*)": "<rootDir>/models/$1",
      "@modules/(.*)": "<rootDir>/modules/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.service.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@common/*": "dist/common",
    "@config/*": "dist/config",
    "@database/*": "dist/database",
    "@entities/*": "dist/entities",
    "@models/*": "dist/models",
    "@modules/*": "dist/modules"
  }
}
